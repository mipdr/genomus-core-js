{
    "name": "genomus-core-js",
    "version": "0.1.2",
    "author": "Miguel Pedregosa",
    "description": "genomus-core bindings for NodeJS",
    "main": "build/index.js",
    "scripts": {
        "test": "node --napi-modules ./test/test_binding.js",
        "build-debug": "node-gyp configure && node-gyp build --debug",
        "install": "node-pre-gyp install --fallback-to-build && tsc"
    },
    "gypfile": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mipdr/genomus-core-js.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mipdr/genomus-core-js/issues"
    },
    "homepage": "https://github.com/mipdr/genomus-core-js#readme",
    "binary": {
        "module_name": "genomus_core_native",
        "module_path": "./build/Release/",
        "host": "https://genomus-bucket.s3-eu-west-1.amazonaws.com/"
    },
    "dependencies": {
        "@mapbox/node-pre-gyp": "^1.0.9",
        "@types/node": "^7.0.22",
        "node-addon-api": "^1.1.0",
        "typescript": "^4.7.4"
    },
    "devDependencies": {
        "aws-sdk": "^2.1189.0"
    }
}
